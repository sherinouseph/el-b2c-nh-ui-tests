 /* SELECT r.date, r.r_id, t.t_id, c.countryCode, c.countryName ,t.TestName, t.url, ROUND(AVG(tr.loadtime), 3) AS loadtime
  from countries c, test t, run r, testrun tr
  where t.t_id IN (37, 41,50,38,44,51)
  AND r.r_id=tr.r_id
  AND t.t_id= tr.t_id
  AND t.c_id=c.c_id
  group by t.t_id
  order by t.t_id;*/

  SELECT t.t_id, c.countryCode, c.countryName , t.url, ROUND(AVG(tr.loadtime), 3 ) AS loadtime
  from countries c, test t, run r, testrun tr where r.date > (SELECT CURDATE()-INTERVAL (WEEKDAY(NOW())+8) DAY)
  AND r.date < (SELECT CURDATE()-INTERVAL WEEKDAY(NOW()) DAY) AND r.r_id=tr.r_id AND t.t_id= tr.t_id AND t.c_id=c.c_id
  group by t.t_id order by t.t_id;



