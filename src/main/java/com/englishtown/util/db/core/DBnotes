 select c.countryCode, c.countryName ,t.TestName, t.url, tr.loadtime
 from countries c, test t, run r, testrun tr
 where r.r_id=tr.r_id
 and t.t_id= tr.t_id
 and t.c_id=c.c_id;
 +-------------+-------------+-----------------------------------+----------+
 | countryCode | countryName | url                               | loadtime |
 +-------------+-------------+-----------------------------------+----------+
 | AM          | Armenia     | http://www.englishtown.com/en-gb/ |     9.90 |
 +-------------+-------------+-----------------------------------+----------+
 CREATE VIEW TEST_STATS AS
 SELECT r.date, r.r_id, t.t_id, c.countryCode, c.countryName ,t.TestName, t.url, tr.loadtime
  from countries c, test t, run r, testrun tr
  where r.r_id=tr.r_id
  and t.t_id= tr.t_id
  and t.c_id=c.c_id
  ORDER BY R.R_ID;

 +---------------------+------+------+-------------+----------------+--------------+-----------------------------------+----------+
 | date                | r_id | t_id | countryCode | countryName    | TestName     | url                               | loadtime |
 +---------------------+------+------+-------------+----------------+--------------+-----------------------------------+----------+
 | 2015-11-11 00:00:00 |    8 |    2 | AM          | Armenia        | UK_HomePage2 | http://www.englishtown.com/en-gb/ |   10.657 |

