  CREATE TABLE IF NOT EXISTS Test (
  t_ID int NOT NULL AUTO_INCREMENT,
  TestName varchar(55) NOT NULL,
  url varchar(255),
  note varchar(255),
  c_id int(5),
  PRIMARY KEY (t_ID),
  FOREIGN KEY (c_id) REFERENCES countries(c_id)
  );

  CREATE TABLE IF NOT EXISTS Run (
  r_ID int NOT NULL AUTO_INCREMENT,
  date DATETIME , //2015-11-11  NO hour ...!
  note varchar(255),
  PRIMARY KEY (r_ID)
  );


 //(3,3)
  CREATE TABLE IF NOT EXISTS TestRun (
  r_ID int,
  t_ID int,
  loadtime FLOAT,
  PRIMARY KEY (r_ID, t_ID),
  FOREIGN KEY (r_ID) REFERENCES Run (r_ID),
  FOREIGN KEY (t_ID) REFERENCES Test(t_ID)
  );

  CREATE VIEW TEST_STATS AS
  SELECT r.date, r.r_id, t.t_id, c.countryCode, c.countryName ,t.TestName, t.url, tr.loadtime
  from countries c, test t, run r, testrun tr
  where r.r_id=tr.r_id
  and t.t_id= tr.t_id
  and t.c_id=c.c_id
  ORDER BY R.R_ID;


  // inserts   /6 AL   YYYY-MM-DD HH:MI:SS "2015-10-25 13:10:25"
  INSERT INTO Test (TestName, url, c_id) VALUES("UK_HomePage", "http://www.englishtown.com/en-gb/", 6);

  INSERT INTO Run (date) VALUES(NOW());

  //TEST
  INSERT INTO TestRun (r_ID, t_ID, loadtime) VALUES(1, 1, 6);
  INSERT INTO TestRun (r_ID, t_ID, loadtime) VALUES(1, 2, 16);

  Select * from Test, Run, TestRun Where TestRun.r_ID=Run.r_id AND TestRun.t_ID=Test.t_id;
  mysql> Select * from Test, Run, TestRun Where TestRun.r_ID=Run.r_id AND TestRun.t_ID=Test.t_id;
