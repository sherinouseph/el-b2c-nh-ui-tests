  select r.r_id, t.t_id, t.url, c.countryCode, c.countryName , tr.loadtime
  from countries c, test t, run r, testrun tr
  where r.r_id=tr.r_id
  and t.t_id= tr.t_id
  and t.c_id=c.c_id;
