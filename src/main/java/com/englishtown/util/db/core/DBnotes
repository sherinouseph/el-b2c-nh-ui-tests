 CREATE TABLE IF NOT EXISTS Test (
 t_ID int NOT NULL AUTO_INCREMENT,
 TestName varchar(55) NOT NULL,
 url varchar(255),
 note varchar(255),
 c_id int(5),
 PRIMARY KEY (t_ID),
 FOREIGN KEY (c_id) REFERENCES countries(c_id)
 );
 UPDATE test SET url = REPLACE(url, 'http', 'https') WHERE url LIKE '%member%';

 //2015-11-11  NO hour ...!
 CREATE TABLE IF NOT EXISTS NikolRun (
   r_ID int NOT NULL AUTO_INCREMENT,
   date DATETIME ,
   PRIMARY KEY (r_ID)
 );


//(3,3)
 CREATE TABLE IF NOT EXISTS TestRun (
 r_ID int,
 t_ID int,
 loadtime FLOAT,
 PRIMARY KEY (r_ID, t_ID),
 FOREIGN KEY (r_ID) REFERENCES Run (r_ID),
 FOREIGN KEY (t_ID) REFERENCES Test(t_ID)
 );//TODO udate with performance timing

 ALTER  TABLE testrun
    ADD ptNavigationStart BIGINT,
    ADD ptConnectStart BIGINT,
    ADD ptRequestStart BIGINT,
    ADD ptResponseStart BIGINT,
    ADD ptDomLoading BIGINT,
    ADD ptDomInteractive BIGINT,
    ADD ptDomComplete BIGINT,
    ADD ptLoadEventEnd BIGINT,
    ADD ptNetworkLatency BIGINT,
    ADD ptPageLoadOnceReceivedFromServer BIGINT,
    ADD ptTimeToDomInteractive BIGINT,
    ADD ptTimeToDomComplete BIGINT,
    ADD ptDomCompleteProcessingTime BIGINT;


        // calculated values
        protected long networkLatency ;                 // responseEnd - fetchStart
        protected long pageLoadOnceReceivedFromServer ; // loadEventEnd - responseEnd
        protected long domCompleteProcessingTime ;      // domComplete - responseEnd
        protected long timeToDomComplete ;              // domComplete - navigationStart;
        protected long timeToSpinnerWay ;               // spinnerGoesWayTime - navigationStart
        protected long timeToDomInteractive;           // domInteractive - navigationStart


 CREATE VIEW TEST_STATS AS
 SELECT r.date, r.r_id, t.t_id, c.countryCode, c.countryName ,t.TestName, t.url, tr.loadtime
 from countries c, test t, run r, testrun tr
 where r.r_id=tr.r_id
 and t.t_id= tr.t_id
 and t.c_id=c.c_id
 ORDER BY R.R_ID;

 // average view   -- created on local :
 // TODO crete on TC, update sql
 CREATE VIEW TEST_STATS_AVG_ALL AS
 SELECT r.r_id, t.t_id, c.countryCode, c.countryName , t.url, AVG(tr.loadtime) Load_Time_Average
 from countries c, test t, run r, testrun tr
 where r.r_id=tr.r_id
 AND t.t_id= tr.t_id
 AND t.c_id=c.c_id
 group by t.t_id;

 // r.date > "2015-11-27" AND r.date < "2015-12-03"

 // inserts   /6 AL   YYYY-MM-DD HH:MI:SS "2015-10-25 13:10:25"
 INSERT INTO Test (TestName, url, c_id) VALUES("UK_HomePage", "http://www.englishtown.com/en-gb/", 6);

